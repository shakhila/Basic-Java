<div class="limiter">
    <div class="container-login100" style="background-image: url('images/bg-01.jpg');">
        <div class="wrap-login100 p-l-55 p-r-55 p-t-65 p-b-54">
            <!-- form starts here -->
            <form class="login100-form validate-form" action="${pageContext.request.contextPath}/staffController" method="post">
					<span class="login100-form-title p-b-49">
						Staff Login
					</span>

                <div class="wrap-input100 validate-input m-b-23" data-validate = "Username is reauired">
                    <span class="label-input100">ID</span>
                    <input class="input100" type="text" name="STAFFID" placeholder="Type your ID">
                    <span class="focus-input100" data-symbol="&#xf206;"></span>
                </div>

                <br>

                <div class="wrap-input100 validate-input" data-validate="Password is required">
                    <span class="label-input100">Password</span>
                    <input class="input100" type="password" name="STAFFPASSWORD" placeholder="Type your password">
                    <input type="hidden" name="action" value="staff-login">
                    <span class="focus-input100" data-symbol="&#xf190;"></span>
                </div>

                <br>

                <div class="text-right p-t-8 p-b-31">
                    <a href="ForgotPassword-1.jsp">
                        Forgot password?
                    </a>
                    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;

                    <a href="StaffCreateEmail.jsp">
                        Create account
                    </a>
                </div>

                <div class="container-login100-form-btn">
                    <div class="wrap-login100-form-btn">
                        <div class="login100-form-bgbtn"></div>
                        <button class="login100-form-btn" type="submit" value="LOGIN"> LOGIN

                        </button>

